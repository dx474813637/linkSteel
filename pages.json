{
	"easycom": {
		"^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"onReachBottomDistance": 50,
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white"
			}
		}, {
			"path": "pages/index/login/login",
			"style": {
				"navigationBarBackgroundColor": "#FFFFFF",
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/login/code",
			"style": {
				"navigationBarBackgroundColor": "#FFFFFF",
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/pan/pan",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white"
			}

		}, {
			"path": "pages/index/pan/panDetail",
			"style": {
				"navigationStyle": "custom",
				"navigationBarTextStyle": "white"
			}

		}, {
			"path": "pages/index/login/register",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false,
				"navigationBarBackgroundColor": "#ffffff"
			}

		}, {
			"path": "pages/index/login/resetPwd",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}, {
			"path": "pages/index/webview/webview",
			"style": {
				"navigationBarTitleText": "",
				"enablePullDownRefresh": false
			}

		}
	],
	"subPackages": [{
		"root": "pages/my",
		"pages": [{
				"path": "user/index",
				"style": {
					"navigationStyle": "custom"
					// "navigationBarTitleText": "我的",
					// "enablePullDownRefresh": false
				}

			}
		]
	}],
	"globalStyle": {
		"navigationBarTitleText": "联钢网",
		"navigationBarTextStyle": "black",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}